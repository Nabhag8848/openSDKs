{
  "name": "@opensdks/sdk-slack",
  "version": "0.0.1",
  "module": "./index.ts",
  "files": [
    "dist",
    "**/*.ts",
    "!**/*.spec.ts"
  ],
  "scripts": {
    "build": "concurrently npm:build:*",
    "build:ts": "tsc -p ./tsconfig.json",
    "clean": "rm -rf ./dist",
    "download": "run-s download:orig download:process",
    "download:orig": "curl --fail https://api.apis.guru/v2/specs/slack.com/1.7.0/openapi.json -o ./slack.orig.oas.json",
    "download:process": "npx tsx slack.openapi.ts | prettier --parser json > ./slack.oas.json",
    "generate": "openapi-typescript ./slack.oas.json --output ./slack.oas.d.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@opensdks/runtime": "workspace:*",
    "concurrently": "^8.2.2",
    "openapi-typescript": "6.7.1",
    "prettier": "^3.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
