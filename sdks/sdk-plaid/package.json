{
  "name": "@opensdks/sdk-plaid",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "**/*.ts",
    "!**/*.spec.ts",
    "!**/*.json"
  ],
  "scripts": {
    "build": "concurrently npm:build:*",
    "build:dts": "mkdir -p dist && cp *.d.ts ./dist",
    "build:json": "mkdir -p dist && npx tsx ../../bin/oasJsonToJs.ts ./ ./dist",
    "build:ts": "tsc -p ./tsconfig.json",
    "clean": "rm -rf ./dist",
    "download": "curl --fail https://raw.githubusercontent.com/plaid/plaid-openapi/master/2020-09-14.yml > ./plaid.oas.yaml",
    "generate": "pnpm generate:oas && pnpm generate:types",
    "generate:oas": "yaml2json ./plaid.oas.yaml | prettier --parser json > ./plaid.oas.json",
    "generate:types": "openapi-typescript ./plaid.oas.json --output plaid.oas.d.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@opensdks/runtime": "workspace:*",
    "concurrently": "^8.2.2",
    "openapi-typescript": "6.7.1",
    "prettier": "3.1.0",
    "yamljs": "0.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
