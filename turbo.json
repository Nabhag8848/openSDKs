{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "download": {
      "outputs": ["*.oas.json"]
    },
    "generate": {
      "dependsOn": ["download"],
      "cache": false,
      "outputs": ["*.oas.ts"]
    },
    "clean": {},
    "build": {
      "dependsOn": ["clean"],
      "outputs": ["dist"]
    },
    "pub": {"cache": false, "dependsOn": ["build"]}
  }
}
